func generate(numRows int) [][]int {
    res := make([][]int, numRows)
    for i:= 0;i<numRows;i++{
        a := make([]int, i+1)
        for j:=0;j<=i;j++{
            if j == 0 || j == i{
                a[j] = 1
            }else{
                a[j] = res[i-1][j-1] + res[i-1][j]
            }
        }
        res[i] = a
    }
    return res
}
