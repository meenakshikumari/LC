func moveZeroes(nums []int)  {
  idx := -1
  i:=0
  for i<len(nums){
    if idx == -1 && nums[i] == 0{
      idx = i
    }else if nums[i] !=0 && idx != -1{
      nums[idx] = nums[i]
      nums[i] = 0
      for idx <=i {
        if nums[idx] == 0 {
          break
        }
        idx++
      }
      continue
    }
    i++
  }
}
