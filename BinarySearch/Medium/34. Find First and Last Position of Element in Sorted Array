// O(logn)
func searchRange(nums []int, target int) []int {
    i:=0
    j:=len(nums)-1
    first := -1
    last := -1
    for i<=j{
        m := (i+j)/2
        //find first occurance
        if nums[m] >= target {
            if nums[m] == target{
                first = m
            }
            j = m-1
        }else{
            i = m+1
        }
    }
    i = 0
    j = len(nums)-1
    for i<=j{
        m := (i+j)/2
        //find last occurance
        if nums[m] > target {
            j = m-1
        }else{
            if nums[m] == target{
                last = m
            }
            i = m+1
        }
    }

    return []int{first, last}
}
