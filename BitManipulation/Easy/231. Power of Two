func isPowerOfTwo(n int) bool {
    // convert to binary O(logN)
    if n < 1 {
        return false
    }
    
    // for n > 1{
    //     if n % 2 == 1 {
    //         return false
    //     }
    //     n = n / 2
    // }
    // return true
    if n & (n-1) != 0{return false} //time: O(1)
    return true
}

