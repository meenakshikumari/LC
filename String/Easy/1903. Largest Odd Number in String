// import "strconv"
func largestOddNumber(num string) string {
    // var largestNum string
    for i:=len(num)-1;i>=0;i--{
        if (num[i]-'0')%2 != 0{
            return num[:i+1]
        }
        // number, _ := strconv.Atoi(num[i-1:i])
        // if number%2 != 0 && num[:i] > largestNum {
            // largestNum = num[:i]
        // }
    }
    return ""
}
