func reverseWords(s string) string {
   func reverseWords(s string) string {
    var start, end int
    var ans strings.Builder
    for i:=len(s)-1; i>=0;i--{
        if end == 0 && s[i] != ' '{ // add the last ptr
            if i ==0 {
                if ans.String() != ""{
                    ans.WriteString(string(" "))
                }
                ans.WriteString(string(s[0:1]))
                return ans.String()
            }
            end = i
            continue
        }
        // add the start ptr
        if start == 0 && end != 0 && (s[i] == ' '|| i==0){
            if ans.String() != ""{
                ans.WriteString(string(" "))
            }
            if i ==0 && s[i] != ' '{
                ans.WriteString(string(s[0:end+1]))
                return ans.String()
            }
            start = i+1
            ans.WriteString(string(s[start:end+1]))
            start = 0
            end = 0
        }
    }
    return ans.String()
}
