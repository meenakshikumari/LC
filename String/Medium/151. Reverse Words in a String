func reverseWords(s string) string {
    var start, end int
    var ans string
    for i:=len(s)-1; i>=0;i--{
        if end == 0 && s[i] != ' '{ // add the last ptr
            if i ==0 {
                if ans != ""{
                    ans += " "
                }
                ans += s[0:1]
                return ans
            }
            end = i
            continue
        }
        // add the start ptr
        if start == 0 && end != 0 && (s[i] == ' '|| i==0){
            if ans != ""{
                ans += " "
            }
            if i ==0 && s[i] != ' '{
                ans += s[0:end+1]
                return ans
            }
            start = i+1
            ans += s[start:end+1]
            start = 0
            end = 0
        }
    }
    return ans
}
